{
  "name": "@graphistry/client-api",
  "version": "3.7.6",
  "description": "Client-side API for interacting with a Graphistry embedded visualization.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "config": {
    "port": 8000,
    "entry": {
      "GraphistryJS": [
        "./src/index.js"
      ]
    },
    "output": {
      "library": null,
      "umdNamedDefine": true
    }
  },
  "scripts": {
    "build": "npm run build:prod && npm run build:cjs && npm run build:esm && npm run build:docs",
    "build:prod": "esbuild --bundle src/index.js --loader:.js=jsx --outfile=lib/index.js --minify --target=chrome58,firefox57,safari11,edge18 && mkdir -p dist && cp lib/index.js dist/GraphistryJS.js",
    "build:dev":  "esbuild --bundle src/index.js --loader:.js=jsx --outfile=www/main.js --sourcemap ",
    "build:cjs":  "esbuild --bundle src/index.js --loader:.js=jsx --jsx=preserve --external:react --external:react-dom --format=cjs --outfile=dist/client-api.cjs.js --minify",
    "build:esm":  "esbuild --bundle src/index.js --loader:.js=jsx --jsx=preserve --external:react --external:react-dom --format=esm --outfile=es/index.js",
    "build:esm2": "esbuild --bundle src/index.js --loader:.js=jsx --jsx=preserve --external:react --external:react-dom --format=esm --outfile=es/index.mjs",
    "build:docs": "rimraf docs && mkdir -p docs && jsdoc -c jsdoc-conf.json -R ./README.md -d docs",
    "prebuild:docs": "rimraf docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/graphistry-js.git"
  },
  "keywords": [
    "graphistry",
    "graph-viz"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "docs"
  ],
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/graphistry/graphistry-js/issues"
  },
  "homepage": "https://github.com/graphistry/graphistry-js/projects/client-api#readme",
  "devDependencies": {
    "@babel/preset-env": "7.16.0",
    "esbuild": "0.13.12",
    "jsdoc": "3.6.7",
    "jsdoc-babel": "~0.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "shallowequal": "1.1.0"
  },
  "dependencies": {
    "@graphistry/falcor-json-graph": "^2.9.10",
    "@graphistry/falcor-model-rxjs": "2.9.11",
    "@graphistry/falcor-socket-datasource": "2.9.11",
    "rxjs": "5.4.3",
    "symbol-observable": "^4.0.0"
  }
}
