{
  "name": "@graphistry/client-api",
  "version": "3.7.6",
  "description": "Client-side API for interacting with a Graphistry embedded visualization.",
  "main": "dist/index.cjs.js",
  "module": "src/index.js",
  "browser": {
    "./index.cjs": "./dist/GraphistryJS.cjs.js",
    "./index.js": "./dist/GraphistryJS.esm.js"
  },
  "exports": {
    "node": {
      "import": "./dist/GraphistryJS.esm.js",
      "require": "./dist/GraphistryJS.cjs.js"
    },
    "default": "./dist/GraphistryJS.esm.js"
  },
  "scripts": {
    "build": "npm run build:prod && npm run build:dev && npm run build:cjs && npm run build:esm",
    "build:prod": "esbuild src/index.js --bundle --format=iife --outfile=dist/GraphistryJS.js --minify --target=chrome58,firefox57,safari11,edge18",
    "build:dev":  "esbuild src/index.js --bundle --format=iife --outfile=dist/GphistryJS.dev.js --sourcemap ",
    "build:cjs":  "esbuild src/index.js --bundle --format=cjs  --outfile=dist/GraphistryJS.cjs.js --minify",
    "build:esm":  "esbuild src/index.js --bundle --format=esm  --outfile=dist/GraphistryJS.esm.js --minify",
    "build:docs": "rimraf docs && mkdir -p docs && jsdoc -c jsdoc-conf.json -R ./README.md -d docs",
    "prebuild:docs": "rimraf docs",
    "lint": "eslint src",
    "clean": "npm run clean:install && npm run clean:build && npm run clean:docs",
    "clean:install": "rimraf node_modules",
    "clean:build": "rimraf dist es libs",
    "clean:docs": "rimraf docs jsdocs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/graphistry-js.git"
  },
  "keywords": [
    "graphistry",
    "graph-viz"
  ],
  "files": [
    "dist",
    "jsdocs",
    "src"
  ],
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/graphistry/graphistry-js/issues"
  },
  "homepage": "https://github.com/graphistry/graphistry-js/projects/client-api#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "7.16.0",
    "esbuild": "0.13.12",
    "eslint": "7.32.0",
    "jsdoc": "3.6.7",
    "jsdoc-babel": "~0.5.0",
    "clean-jsdoc-theme": "3.2.10",
    "rimraf": "3.0.2",
    "shallowequal": "1.1.0"
  },
  "dependencies": {
    "@graphistry/falcor-json-graph": "^2.9.10",
    "@graphistry/falcor-model-rxjs": "2.9.11",
    "@graphistry/falcor-socket-datasource": "2.9.11",
    "rxjs": ">=7.2.0"
  }
}
