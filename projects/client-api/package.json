{
  "name": "@graphistry/client-api",
  "version": "3.7.6",
  "description": "Client-side API for interacting with a Graphistry embedded visualization.",
  "jsnext:main": "es/index.js",
  "config": {
    "port": 8000,
    "entry": {
      "GraphistryJS": [
        "./src/index.js"
      ]
    },
    "output": {
      "library": null,
      "umdNamedDefine": true
    }
  },

  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "docs",
    "README.md"
  ],

  "scripts": {
    "build": "npm run build:js && npm run build:docs",
    "build:js": "npm run build:cjs && npm run build:esm && npm run build:esm2 && npm run build:iifi",
    "build:cjs":  "esbuild --bundle src/index.js --sourcemap --target=es6 --format=cjs --outfile=dist/client-api.cjs.js --minify",
    "build:esm":  "esbuild --bundle src/index.js --sourcemap --target=es6 --format=esm --outfile=dist/index.js",
    "build:esm2": "esbuild --bundle src/index.js --sourcemap --target=es6 --format=esm --outfile=dist/index.mjs",
    "build:iifi": "esbuild --bundle src/index.js --sourcemap --target=es6 --format=esm --outfile=dist/index.iifi.js --global-name=GraphistryJS",
    "build:docs": "rimraf docs && mkdir -p docs && jsdoc -c jsdoc-conf.json -R ./README.md -d docs",
    "lock:fix": "npm i --package-lock-only",
    "prebuild:docs": "rimraf docs",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/graphistry-js.git"
  },
  "keywords": [
    "graphistry",
    "graph-viz"
  ],
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/graphistry/graphistry-js/issues"
  },
  "homepage": "https://github.com/graphistry/graphistry-js/projects/client-api#readme",
  "devDependencies": {
    "@babel/preset-env": "7.16.0",
    "esbuild": "0.13.12",
    "eslint": "7.32.0",
    "jsdoc": "3.6.7",
    "jsdoc-babel": "~0.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "shallowequal": "1.1.0"
  },
  "dependencies": {
    "@graphistry/falcor-json-graph": "^2.9.10",
    "@graphistry/falcor-model-rxjs": "2.9.11",
    "@graphistry/falcor-socket-datasource": "2.9.11",
    "rxjs": "5.4.3",
    "symbol-observable": "^4.0.0"
  }
}
