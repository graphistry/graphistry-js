{
  "name": "@graphistry/client-api-react",
  "version": "3.7.6",
  "description": "Embed a Graphistry visualization via React",
  "type": "commonjs",
  "main": "./dist/GraphistryReact.cjs.js",
  "exports": {
    "import": "./dist/GraphistryReact.esm.js",
    "require": "./dist/GraphistryReact.cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/graphistry-js.git"
  },
  "homepage": "https://github.com/graphistry/graphistry-js/projects/client-api-react#readme",
  "scripts": {
    "build": "npm run build:prod && npm run  build:dev && npm run  build:cjs && npm run build:esm",
    "build:prod": "esbuild src/index.js --bundle --loader:.js=jsx --format=iife --outfile=dist/GraphistryReact.js --minify --target=chrome58,firefox57,safari11,edge18",
    "build:dev":  "esbuild src/index.js --bundle --loader:.js=jsx --format=iife --outfile=dist/GraphistryReact.dev.js --sourcemap ",
    "build:cjs":  "esbuild src/index.js --bundle --loader:.js=jsx --format=cjs  --outfile=dist/GraphistryReact.cjs.js --minify",
    "build:esm":  "esbuild src/index.js --bundle --loader:.js=jsx --format=esm  --outfile=dist/GraphistryReact.esm.js",
    "build:docs": "rimraf docs && jsdoc -c jsdoc-conf.json -R ./README.md -d docs",
    "prebuild:docs": "rimraf docs",
    "lint": "eslint src",
    "clean": "npm run clean:install && npm run clean:build && npm run clean:docs",
    "clean:install": "rimraf node_modules",
    "clean:build": "rimraf dist es libs",
    "clean:docs": "rimraf docs jsdocs docs-build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build -s ./assets"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "docs",
    "assets"
  ],
  "keywords": [],
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "ISC",
  "dependencies": {
    "@graphistry/client-api": "^3.7.6",
    "prop-types": ">=15.6.0",
    "shallowequal": "1.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addons": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-storysource": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "babel-loader": "^8.2.3",
    "esbuild": "0.13.12",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.26.1",
    "jsdoc": "3.6.7",
    "jsdoc-babel": "~0.5.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "rimraf": "3.0.2"
  }
}
