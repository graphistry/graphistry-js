{
  "name": "@graphistry/client-api-react",
  "version": "3.7.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/graphistry-js.git"
  },
  "homepage": "https://github.com/graphistry/graphistry-js/projects/client-api-react#readme",


  "private": true,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "files": [
    "assets",
    "dist",
    "docs",
    "README.md"
  ],

  "description": "Embed a Graphistry visualization via React",
  "moduleEslint": "dist/index.pure.full.js",

  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "npm run build:esbuild && npm run build:rollup",

    "lock:fix": "npm i --package-lock-only",

    "clean": "npm run clean:builds && npm run clean:node_modules",
    "clean:node_modules": "rm -rf node_modules && rm -rf .cache",
    "clean:builds": "rm -rf dist && rm -rf www && rm -rf .next",
    
    "build:rollup": "rollup -c",
    "build:rollup-watch": "rollup -c -w",

    "build:esbuild": "npm run build:dev && npm run build:linked && npm run build:full",
    "build:dev": "esbuild --bundle src/index.js  --target=es6 --loader:.js=jsx --outfile=www/main.js --sourcemap",
    "build:pure": "npm run build:cjs:js && npm run build:esm:js && npm run build:iife:js",

    "build:linked": "npm run build:cjs:js && npm run build:esm:js && npm run build:iife:js",
    "build:cjs:js": "esbuild --bundle src/index.js      --sourcemap --target=es6 --loader:.js=jsx --format=cjs  --external:react --external:react-dom --outfile=dist/index.cjs.js",
    "build:esm:js": "esbuild --bundle src/index.js      --sourcemap --target=es6 --loader:.js=jsx --format=esm  --external:react --external:react-dom --outfile=dist/index.esm.js",
    "build:iife:js": "esbuild --bundle src/index.js     --sourcemap --target=es6 --loader:.js=jsx --format=iife --external:react --external:react-dom --outfile=dist/index.iife.js --global-name=Graphistry",

    "build:full": "npm run build:cjs:js:full && npm run build:esm:js:full && npm run build:iife:js:full",
    "build:cjs:js:full": "esbuild --bundle src/index.js  --sourcemap --target=es6 --loader:.js=jsx --format=cjs  --outfile=dist/index.full.cjs.js",
    "build:esm:js:full": "esbuild --bundle src/index.js  --sourcemap --target=es6 --loader:.js=jsx --format=esm  --outfile=dist/index.full.esm.js",
    "build:iife:js:full": "esbuild --bundle src/index.js --sourcemap --target=es6 --loader:.js=jsx --format=iife --outfile=dist/index.full.iife.js --global-name=Graphistry",

    "build:docs": "rimraf docs && jsdoc -c jsdoc-conf.json -R ./README.md -d docs",
    "prebuild:docs": "rimraf docs",
    "lint": "eslint src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build -s ./assets"
  },
  "filesESBuild": [
    "dist",
    "docs",
    "assets"
  ],
  "keywords": [],
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "ISC",
  "dependencies": {
    "@graphistry/client-api": "^3.7.6",
    "prop-types": ">=15.6.0",
    "shallowequal": "1.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "react-scripts": "4.0.3"
  },

  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {

    "@babel/core": "^7.16.0",

    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.60.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",

    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",

    "@storybook/addons": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-storysource": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "babel-loader": "^8.2.3",
    "esbuild": "0.13.12",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.26.1",
    "jsdoc": "3.6.7",
    "jsdoc-babel": "~0.5.0",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "rimraf": "3.0.2"
  }
}
