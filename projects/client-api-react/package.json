{
  "name": "@graphistry/client-api-react",
  "version": "4.4.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphistry/graphistry-js.git"
  },
  "homepage": "https://github.com/graphistry/graphistry-js/projects/client-api-react#readme",
  "private": false,
  "main": "dist/index.cjs.min.js",
  "module": "dist/index.esm.min.js",
  "source": "src/index.js",
  "files": [
    "assets",
    "dist",
    "docs",
    "README.md"
  ],
  "description": "Embed a Graphistry visualization via React",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "npm run build:rollup && npm run build:esbuild",
    "lock:fix": "npm i --package-lock-only",
    "clean": "npm run clean:install && npm run clean:builds && npm run clean:cache && npm run clean:docs",
    "clean:install": "rm -rf node_modules",
    "clean:builds": "rm -rf dist && rm -rf www && rm -rf .next",
    "clean:cache": "rm -rf .cache",
    "clean:docs": "rimraf docs jsdocs docs-build",
    "build:rollup": "rollup -c",
    "build:rollup-watch": "rollup -c -w",
    "build:esbuild": "npm run build:dev && npm run build:linked && npm run build:full",
    "build:dev": "esbuild --bundle src/index.js  --target=es6 --loader:.js=jsx --outfile=www/main.js --sourcemap",
    "build:linked": "npm run build:linked:js && npm run build:linked:min:js",
    "build:linked:js": "npm run build:cjs:js && npm run build:esm:js && npm run build:umd:js && npm run build:iife:js",
    "build:cjs:js": "esbuild --bundle src/index.js       --sourcemap --target=es6 --loader:.js=jsx --format=cjs  --external:react --external:react-dom --outfile=dist/index.cjs.js",
    "build:esm:js:broken": "esbuild --bundle src/index.js       --sourcemap --target=es6 --loader:.js=jsx --format=esm  --external:react --external:react-dom --outfile=dist/index.esm.js",
    "build:esm:js": "echo 'relying on rollup -c'",
    "build:umd:js": "echo 'relying on rollup -c'",
    "build:iife:js": "esbuild --bundle src/index.js      --sourcemap --target=es6 --loader:.js=jsx --format=iife --external:react --external:react-dom --outfile=dist/index.iife.js --global-name=Graphistry",
    "build:linked:min:js": "npm run build:cjs:min:js && npm run build:esm:min:js && npm run build:umd:min:js && npm run build:iife:min:js",
    "build:cjs:min:js": "esbuild --bundle src/index.js       --minify --sourcemap --target=es6 --loader:.js=jsx --format=cjs  --external:react --external:react-dom --outfile=dist/index.cjs.min.js",
    "build:esm:min:js:broken": "esbuild --bundle src/index.js       --minify --sourcemap --target=es6 --loader:.js=jsx --format=esm  --external:react --external:react-dom --outfile=dist/index.esm.min.js",
    "build:esm:min:js": "echo 'relying on rollup -c'",
    "build:umd:min:js": "echo 'relying on rollup -c'",
    "build:iife:min:js": "esbuild --bundle src/index.js      --minify --sourcemap --target=es6 --loader:.js=jsx --format=iife --external:react --external:react-dom --outfile=dist/index.iife.min.js --global-name=Graphistry",
    "build:full": "npm run build:full:js && npm run build:full:min:js",
    "build:full:js": "npm run build:cjs:js:full && npm run build:esm:js:full && npm run build:iife:js:full",
    "build:cjs:js:full": "esbuild --bundle src/index.js  --sourcemap --target=es6 --loader:.js=jsx --format=cjs  --outfile=dist/index.full.cjs.js",
    "build:esm:js:full": "esbuild --bundle src/index.js  --sourcemap --target=es6 --loader:.js=jsx --format=esm  --outfile=dist/index.full.esm.js",
    "build:iife:js:full": "esbuild --bundle src/index.js --sourcemap --target=es6 --loader:.js=jsx --format=iife --outfile=dist/index.full.iife.js --global-name=Graphistry",
    "build:full:min:js": "npm run build:cjs:js:full && npm run build:esm:js:full && npm run build:iife:js:full",
    "build:cjs:min:js:full": "esbuild --bundle src/index.js  --minify --sourcemap --target=es6 --loader:.js=jsx --format=cjs  --outfile=dist/index.full.cjs.min.js",
    "build:esm:min:js:full": "esbuild --bundle src/index.js  --minify --sourcemap --target=es6 --loader:.js=jsx --format=esm  --outfile=dist/index.full.esm.min.js",
    "build:iife:min:js:full": "esbuild --bundle src/index.js --minify --sourcemap --target=es6 --loader:.js=jsx --format=iife --outfile=dist/index.full.iife.min.js --global-name=Graphistry",
    "build:docs": "rimraf docs && jsdoc -c jsdoc-conf.json -R ./README.md -d docs",
    "prebuild:docs": "rimraf docs",
    "lint": "eslint src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build -s ./assets"
  },
  "filesESBuild": [
    "dist",
    "docs",
    "assets"
  ],
  "keywords": [],
  "author": "Graphistry, Inc <https://graphistry.com>",
  "license": "ISC",
  "dependencies": {
    "@graphistry/client-api": "^4.4.1",
    "crypto-browserify": "3.12.0",
    "prop-types": ">=15.6.0",
    "shallowequal": "1.1.0",
    "uuid": "9.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-storysource": "^6.4.22",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.1.21",
    "@storybook/theming": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "esbuild": "0.14.39",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.26.1",
    "jsdoc": "^4.0.2",
    "jsdoc-babel": "~0.5.0",
    "npm-run-all": "^4.1.5",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-scripts": "^5.0.1",
    "rimraf": "3.0.2",
    "rollup": "^2.72.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "gitHead": "949ca04aa87b399e0185c64d7a6810780a841deb"
}
